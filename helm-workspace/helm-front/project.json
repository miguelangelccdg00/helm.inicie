{
  "name": "helm-front",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "helm-front/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/helm-front",
        "index": "helm-front/src/index.html",
        "main": "helm-front/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "helm-front/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "helm-front/src/favicon.ico",
          "helm-front/src/assets"
        ],
        "styles": ["helm-front/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "buildTarget": "helm-front:build",
        "open": true
        
      },
      "configurations": {
        "production": {
          "buildTarget": "helm-front:build:production"
        },
        "development": {
          "buildTarget": "helm-front:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "helm-front:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "helm-front/**/*.ts",
          "helm-front/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "helm-front/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}