{
  "npmScope": "helm-workspace",
  "affected": {
    "defaultBase": "main"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runner",
      "options": {
        "maxParallel": 3,
        "cacheableOperations": ["build", "test", "lint"]
      }
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:application": {
      "style": "scss"
    },
    "@nrwl/express:application": {
      "buildable": true
    }
  }
}