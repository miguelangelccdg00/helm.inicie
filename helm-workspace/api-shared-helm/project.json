{
  "name": "api-shared-helm",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "api-shared-helm/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "api-shared-helm:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "api-shared-helm:build:development"
        },
        "production": {
          "buildTarget": "api-shared-helm:build:production"
        }
      }
    }
  },
  "tags": []
}
